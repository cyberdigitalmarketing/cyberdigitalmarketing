{
  "version": 2,
  "functions": {
    "api/contact.ts": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/debug.ts": {
      "memory": 128,
      "maxDuration": 5
    }
  },
  "routes": [
    { "src": "/api/contact", "dest": "/api/contact.ts" },
    { "src": "/api/debug", "dest": "/api/debug.ts" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}